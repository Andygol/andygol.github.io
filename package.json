    },
    "dependencies": {
        "bootstrap": "5.3.8",
        "simple-icons": "13.16.0"
    },
    "devDependencies": {
        "autoprefixer": "10.4.21",
        "browser-sync": "3.0.4",
        "chokidar": "4.0.3",
        "concurrently": "9.2.1",
